{
  "globals": {
    "JSX": true
  },
  "plugins": ["@tanstack/query"],
  "extends": [
    "next/core-web-vitals",
    "next",
    "prettier",
    "plugin:@tanstack/eslint-plugin-query/recommended"
  ],
  "rules": {
    "space-before-function-paren": "off",
    "react/jsx-closing-bracket-location": "off",
    "react/jsx-curly-newline": "off"
  }
}
